{
  "manifest_version": 2,
  "name": "demo",
  "version": "1.0.0",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "img/icon.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": [
        "js/jquery.js",
        "js/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "options_page": "index.html",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}